        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
          "expr": "boilerhouse_pool_size{pool_id=~\"$pool_id\"}",
          "format": "table",
          "instant": true
        },
        {
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
          "expr": "boilerhouse_pool_min_size{pool_id=~\"$pool_id\"}",
          "format": "table",
          "instant": true
        },
        {
          "refId": "C",
          "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
          "expr": "boilerhouse_pool_max_size{pool_id=~\"$pool_id\"}",
          "format": "table",
          "instant": true
        },
        {
          "refId": "D",
          "datasource": { "type": "prometheus", "uid": "DS_PROMETHEUS" },
          "expr": "boilerhouse_pool_borrowed{pool_id=~\"$pool_id\"}",
          "format": "table",
          "instant": true
