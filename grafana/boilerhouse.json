          "expr": "rate(boilerhouse_affinity_hits_total{pool_id=~\"$pool_id\"}[$__rate_interval]) / (rate(boilerhouse_affinity_hits_total{pool_id=~\"$pool_id\"}[$__rate_interval]) + rate(boilerhouse_affinity_misses_total{pool_id=~\"$pool_id\"}[$__rate_interval])) * 100",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "pool_id": { "operation": "groupby", "aggregations": [] },
              "workload_id": { "operation": "groupby", "aggregations": [] },
              "Value #A": { "operation": "aggregate", "aggregations": ["lastNotNull"] },
              "Value #B": { "operation": "aggregate", "aggregations": ["lastNotNull"] },
              "Value #C": { "operation": "aggregate", "aggregations": ["lastNotNull"] },
              "Value #D": { "operation": "aggregate", "aggregations": ["lastNotNull"] },
              "Value #E": { "operation": "aggregate", "aggregations": ["lastNotNull"] },
              "Value #F": { "operation": "aggregate", "aggregations": ["lastNotNull"] },
              "Value #G": { "operation": "aggregate", "aggregations": ["lastNotNull"] }
            }
          }
        },
        {
          "id": "organize",
          "options": {
            "indexByName": {
              "pool_id": 0,
              "workload_id": 1,
              "Value #A (lastNotNull)": 2,
              "Value #B (lastNotNull)": 3,
              "Value #C (lastNotNull)": 4,
              "Value #D (lastNotNull)": 5,
              "Value #E (lastNotNull)": 6,
              "Value #F (lastNotNull)": 7,
              "Value #G (lastNotNull)": 8
            },
            "renameByName": {
              "pool_id": "Pool ID",
              "workload_id": "Workload",
              "Value #A (lastNotNull)": "Total",
              "Value #B (lastNotNull)": "Min",
              "Value #C (lastNotNull)": "Max",
              "Value #D (lastNotNull)": "Claimed",
              "Value #E (lastNotNull)": "Idle",
              "Value #F (lastNotNull)": "Utilization %",
              "Value #G (lastNotNull)": "Affinity Hit %"
            }
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
